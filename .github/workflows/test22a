
on:
  push

jobs:
  matlab-job-22a:
    runs-on: ubuntu-latest
    steps:

      - name: Check out repository
        uses: actions/checkout@v2

      - name: Setup MATLAB
        uses: matlab-actions/setup-matlab@v1
        with:
          release: R2022a

      - name: Run tests
        uses: matlab-actions/run-command@v1
        with:
          command: pwd
